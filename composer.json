{
    "name": "flarumarabic/arabic",
    "type": "flarum-extension",
    "description": "Arabic language pack.",
    "keywords": [
        "locale"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/idtechsa/flarumarabic/issues",
        "source": "https://github.com/idtechsa/flarumarabic"
    },
    "homepage": "https://github.com/idtechsa/",
    "funding": [
        {
            "type": "website",
            "url": "https://github.com/idtechsa"
        }
    ],
    "authors": [
        {
            "name": "idtechsa",
            "homepage": "https://github.com/idtechsa"
        }
    ],
    "support": {
        "issues": "https://github.com/idtechsa/flarumarabic/issues",
        "source": "https://github.com/idtechsa/flarumarabic/"
    },
   "require": {
        "flarum/core": "^1.0.0"
    },
    "minimum-stability": "dev",
    "autoload": {
		"psr-4": {
			"Flarumarabic\\Arabic\\": "src/"
		}
	},
    "extra": {
        "branch-alias": {
            "dev-master": "1.10.x-dev"
        },
        "authors": [
            {
                "name": "idtechsa",
                "role": "Translator"
            }
